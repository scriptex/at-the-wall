{
  "name": "at-the-wall",
  "version": "1.0.0",
  "description": "A Game of Thrones scene coded in HTML and CSS",
  "scripts": {
    "build": "webpack --env.NODE_ENV=production",
    "start": "webpack --watch --env.NODE_ENV=development",
    "optisize": "optisize --src=\"./assets/images\"",
    "html": "php index.php > index.html",
    "critical": "critical index.html > assets/dist/critical.css",
    "rm-html": "rm index.html",
    "pwa": "create-pwa --icon=\"./assets/images/favicon/icon.png\"",
    "prod": "yarn build && yarn html && yarn critical && yarn rm-html"
  },
  "keywords": [
    "GoT",
    "Game of Thrones",
    "scene",
    "pure css"
  ],
  "author": "Atanas Atanasov <scriptex.bg@gmail.com> (https://atanas.info)",
  "repository": "https://github.com/scriptex/at-the-wall",
  "bugs": {
    "url": "https://github.com/scriptex/at-the-wall/issues",
    "email": "scriptex.bg@gmail.com"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@three11/optisize": "0.6.0",
    "autoprefixer": "9.4.3",
    "browser-sync": "2.26.3",
    "browser-sync-webpack-plugin": "2.2.2",
    "clean-webpack-plugin": "1.0.0",
    "create-pwa": "0.4.0",
    "critical": "1.3.4",
    "css-loader": "2.0.1",
    "cssnano": "4.1.7",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "node-sass": "4.11.0",
    "node-sass-magic-importer": "5.2.0",
    "postcss-easy-import": "3.0.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-merge-rules": "4.0.2",
    "postcss-normalize": "7.0.1",
    "postcss-url": "8.0.0",
    "postcss-utilities": "0.8.0",
    "postcss-watch-folder": "0.1.2",
    "sass-loader": "7.1.0",
    "webpack": "4.28.0",
    "webpack-cli": "3.1.2",
    "yargs": "12.0.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "main": "assets/styles/main.scss",
  "private": true
}
